#include <iostream>
使用命名空间标准；

#define MAX 20

// 事件暂存在res数组中
内部资源[20]；
int p_res = 0;

// 将n进行划分
int resolve(int n, int min_factor = 1);

int main() {
	而 (1) {
		国际n;
		cin >> n;
		cout << "总资源数：\t" << resolve(n) << endl;
	}
	返回0；
}

int 解析（int n，int min_factor）{
	如果 (min_factor > n)
		返回0；
	如果（min_factor == n）{
		res[p_res] = min_factor;
		p_res++;
		for (int i = 0; i < p_res; i++)
			cout << res[i] << " ";
		cout<<endl;
		p_res--;
		返回 1；
	}
	res[p_res] = min_factor;
	p_res++;
	int k = resolve(n - min_factor, min_factor);
	p_res--;

	int s = resolve(n, min_factor + 1);
	返回 k + s;
}
